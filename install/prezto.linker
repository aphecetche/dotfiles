#!/usr/bin/env sh

echo -n "~/github.com/aphecetche/prezto:~/dotfiles/prezto|"

# link for prezto files
# mind the order, first the $HOME/.zpresto then the rest...

echo -n "~/dotfiles/prezto:$HOME/.zprezto|"

for rcfile in $(find "${ZDOTDIR:-$HOME}"/.zprezto/runcoms/ -name z*); do
    echo -n "$rcfile:${ZDOTDIR:-$HOME}/.$(basename $rcfile)|"
done

