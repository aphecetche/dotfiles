#!/usr/bin/env sh

# from ~/dotfiles/config/xorg to their _dot_ equivalent in $HOME

# don't have to setup Xorg on Mac
if [ "$(uname)" != "Darwin" ]; then

    for file in $(find $HOME/dotfiles/config/xorg -type f -name '[Xx]*'); do
        target=~/.$(basename $file)
        echo -n "$file:$target|"
    done
fi
